		"sql",
		"server",
		"msnodesql",
		"sqlserver",
		"tds",
		"node-tds",
		"tedious",
		"node-sqlserver",
		"sqlserver",
		"msnodesqlv8",
		"azure",
		"node-mssql"
	],
	"version": "3.3.1",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/jholster/node-mssql"
	},
	"license": "MIT",
	"dependencies": {
		"tedious": "~1.14.0",
		"generic-pool": "^2.2.0",
		"promise": "^7.0.1"
